{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": "..",
    "rootDir": "..",
    "jsx": "react-jsx",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictPropertyInitialization": false,
    "paths": {
      "@/*": ["src/*"],
      "@sruim/pbr-visualizer-sdk": ["src/index.ts"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["src/single.ts"]
}
